{
  "name": "react-isomorphic-scriptloader",
  "version": "3.0.0",
  "description": "Load scripts with ease",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/archcorsair/react-isomorphic-scriptloader"
  },
  "keywords": [
    "react",
    "scripts",
    "loader"
  ],
  "author": "Daniel S <archcorsair@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/archcorsair/react-isomorphic-scriptloader/issues"
  },
  "peerDependencies": {
    "react": ">=16.8.0 || >=19.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.0.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^7.0.0",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18"
  }
}
